<UserControl x:Class="Visualizer.UserControls.ChipKickTunnerUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="50" Width="108" BorderThickness="1" BorderBrush="Green">
    <Grid Background="Black">
        <Slider Margin="0,23,0,0"
                Name="slider"
                VerticalAlignment="Top"
                Minimum="-1000"
                Maximum="1000"
                SmallChange="0.1"
                Height="24"
               />
        <TextBox Height="23"
                 Name="textBox1"
                 VerticalAlignment="Top"
                 />
    </Grid>
</UserControl>
